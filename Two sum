class Solution {
    public int[] twoSum(int[] nums, int target) {
        int i;
        int j=0;
        for(i=1;i<nums.length;i++){
            for( j=0;j<i;j++){
                if(nums[j]+nums[i]== target)
                return new int[]{j,i};
            }
        }
         return new int[]{j,i};
        
    }
   
}
